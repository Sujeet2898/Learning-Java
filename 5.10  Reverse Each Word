import java.util.*;
public class Main{
	public static void main(String[] args) {
	   Scanner scn = new Scanner(System.in);
	   String s = scn.nextLine();
       String [] temp = s.split(" ");
       String revString = "";
       
       for (int i = 0; i < temp.length; i++) {
           String word = temp[i];
           String revWord = "";
           
           for (int j = temp.length-1; i >= 0; i--){
           revWord += word.charAt(j);
         } 
         revString += revWord + " ";
       }
       System.out.println(revString);
	}
}
