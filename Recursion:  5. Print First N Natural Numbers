
1 to n

import java.util.*;
public class Main
{
    public static void print1toN(int n) {
        if (n == 0) {
            return;
        }
        
        print1toN(n - 1);
        System.out.println(n);
        
    }
	public static void main(String[] args) {
       print1toN(10);
	}
}








n to 1

import java.util.*;
public class Main
{
    public static void print1toN(int n) {
        if (n == 0) {
            return;
        }
        
        System.out.println(n);
        print1toN(n - 1);
    
    }
	public static void main(String[] args) {
       print1toN(10);
	}
}
