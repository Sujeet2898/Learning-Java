import java.util.*;
public class Main{
	public static void main(String[] args) {
		Scanner scn = new Scanner(System.in);
		String s = scn.nextLine();
		s = s.toUpperCase();
		
		int vowel = 0;
		int cons = 0;
		
		for (int i = 0; i < s.length(); i++) {
		    char ch = s.charAt(i);
		    if (ch != ' ') {
		        if (ch == 'A' || ch == 'E' || ch == 'I' || ch == 'O' || ch == 'U') {
		            vowel++;
		        } else {
		            cons++;
		        }
		    }
		}
		System.out.println("Total Vowels: " + vowel + " And Total Consonants: " + cons);
	}
}

input:
sweet is tasty

output:
Total Vowels: 4 And Total Consonants: 8

